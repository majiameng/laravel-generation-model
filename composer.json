{
    "name": "tinymeng/laravel",
    "description": "tinymeng Components for Laravel Framework code generation.",
    "keywords": ["tinymeng", "laravel"],
    "homepage": "http://majiameng.com",
    "support": {
        "issues": "https://github.com/majiameng/laravel-generation-model/issues",
        "source": "https://github.com/majiameng/laravel-generation-model"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "tinymeng",
            "email": "666@majiameng.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": ">=2.5",
        "illuminate/support": ">=5.1",
        "illuminate/database": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/filesystem": ">=5.1",
        "illuminate/console": ">=5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": ">=1.4",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "tinymeng\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "tinymeng\\Laravel\\Coders\\CodersServiceProvider"
            ]
        }
    }
}
